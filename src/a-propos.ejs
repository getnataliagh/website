<!DOCTYPE html>
<html lang="<%= locals.lang %>">
<head>
  <title><%= locals.pageMeta.title %></title>
  <meta name="description" content="<%= locals.pageMeta.description %>">
  <%- include('./includes/_head.ejs') %>
</head>
<body class="min-h-screen flex flex-col">

<%- include('./includes/_header.ejs') %>

<main class="flex-grow">

<!-- Hero Section -->
<section class="py-24 md:py-32 bg-[#030014] relative overflow-hidden">
  <!-- Animated background -->
  <div class="absolute inset-0">
    <div class="absolute top-0 left-1/4 w-[600px] h-[600px] bg-gradient-to-r from-primary/30 to-transparent rounded-full blur-[120px] animate-pulse-slow"></div>
    <div class="absolute bottom-0 right-1/4 w-[500px] h-[500px] bg-gradient-to-l from-secondary/20 to-transparent rounded-full blur-[100px] animate-pulse-slow" style="animation-delay: 2s;"></div>
    <div class="absolute inset-0 bg-[linear-gradient(rgba(92,57,238,0.03)_1px,transparent_1px),linear-gradient(90deg,rgba(92,57,238,0.03)_1px,transparent_1px)] bg-[size:60px_60px]"></div>
  </div>

  <div class="container-content relative z-10">
    <div class="text-center max-w-4xl mx-auto">
      <div class="inline-flex items-center gap-2 px-4 py-2 rounded-full bg-primary/10 border border-primary/20 mb-8 animate-on-scroll">
        <span class="w-2 h-2 bg-primary rounded-full animate-pulse"></span>
        <span class="text-primary font-medium text-sm tracking-wide"><%= locals.about.hero.badge %></span>
      </div>
      <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold text-white mb-6 leading-tight animate-on-scroll">
        <%= locals.about.hero.title %> <span class="bg-gradient-to-r from-primary via-purple-400 to-secondary bg-clip-text text-transparent">Natalia</span>
      </h1>
      <p class="text-xl text-white/60 max-w-2xl mx-auto animate-on-scroll">
        <%= locals.about.hero.subtitle %>
      </p>
    </div>
  </div>
</section>

<!-- Mission Section -->
<section class="section bg-white">
  <div class="container-content">
    <div class="grid grid-cols-1 lg:grid-cols-2 gap-16 items-center">
      <div class="animate-on-scroll">
        <div class="inline-flex items-center gap-2 px-3 py-1.5 rounded-full bg-primary/10 border border-primary/20 mb-6">
          <span class="w-1.5 h-1.5 bg-primary rounded-full"></span>
          <span class="text-primary text-xs font-semibold uppercase tracking-wider"><%= locals.about.mission.badge %></span>
        </div>
        <h2 class="text-3xl md:text-4xl font-bold text-police-titre mb-6">
          <%= locals.about.mission.title %> <span class="text-gradient"><%= locals.about.mission.titleHighlight %></span>
        </h2>
        <p class="text-lg text-police-grey mb-4">
          <%- locals.about.mission.paragraph1 %>
        </p>
        <p class="text-police-grey mb-4">
          <%= locals.about.mission.paragraph2 %>
        </p>
        <p class="text-police-grey">
          <%= locals.about.mission.paragraph3 %>
        </p>
      </div>
      <div class="animate-on-scroll--scale">
        <div class="bg-gradient-to-br from-[#030014] to-primary/20 rounded-3xl p-10 text-center relative overflow-hidden">
          <div class="absolute inset-0 bg-[linear-gradient(rgba(92,57,238,0.05)_1px,transparent_1px),linear-gradient(90deg,rgba(92,57,238,0.05)_1px,transparent_1px)] bg-[size:30px_30px]"></div>
          <div class="relative z-10">
            <div class="text-7xl md:text-8xl font-bold bg-gradient-to-r from-primary via-purple-400 to-secondary bg-clip-text text-transparent mb-4">
              <%= locals.about.mission.stat %>
            </div>
            <p class="text-white/70 text-lg">
              <%= locals.about.mission.statText %><br>
              <span class="text-primary font-semibold"><%= locals.about.mission.statHighlight %></span>
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- Timeline Section -->
<section class="section bg-gray-50">
  <div class="container-content">
    <div class="text-center mb-16 animate-on-scroll">
      <div class="inline-flex items-center gap-2 px-4 py-2 rounded-full bg-primary/10 border border-primary/20 mb-6">
        <span class="w-2 h-2 bg-primary rounded-full animate-pulse"></span>
        <span class="text-primary font-medium text-sm tracking-wide"><%= locals.about.timeline.badge %></span>
      </div>
      <h2 class="text-3xl md:text-4xl font-bold text-police-titre">
        <%= locals.about.timeline.title %> <span class="text-gradient">Natalia</span>
      </h2>
    </div>

    <div class="relative max-w-4xl mx-auto">
      <!-- Timeline line -->
      <div class="absolute left-8 md:left-1/2 top-0 bottom-0 w-0.5 bg-gradient-to-b from-primary via-purple-500 to-secondary"></div>

      <!-- Timeline items -->
      <div class="space-y-12">
        <% var colors = ['primary', 'purple-500', 'emerald-500', 'secondary']; %>
        <% var textColors = ['text-primary', 'text-purple-500', 'text-emerald-500', 'text-secondary']; %>
        <% locals.about.timeline.events.forEach(function(event, index) { %>
        <% var isEven = index % 2 === 0; %>
        <div class="relative flex items-start md:items-center" data-stagger>
          <% if (isEven) { %>
          <div class="flex-1 md:text-right md:pr-12 order-2 md:order-1 pl-16 md:pl-0">
            <div class="bg-white rounded-2xl p-6 shadow-lg border border-gray-100 animate-on-scroll">
              <div class="<%= textColors[index] %> font-bold text-lg mb-2"><%= event.date %></div>
              <h3 class="text-xl font-bold text-police-titre mb-2"><%= event.title %></h3>
              <p class="text-police-grey"><%= event.description %></p>
            </div>
          </div>
          <div class="absolute left-8 md:left-1/2 -translate-x-1/2 w-4 h-4 bg-<%= colors[index] %> rounded-full border-4 border-white shadow-lg z-10"></div>
          <div class="flex-1 md:pl-12 order-1 md:order-2 hidden md:block"></div>
          <% } else { %>
          <div class="flex-1 md:pr-12 order-2 hidden md:block"></div>
          <div class="absolute left-8 md:left-1/2 -translate-x-1/2 w-4 h-4 bg-<%= colors[index] %> rounded-full border-4 border-white shadow-lg z-10"></div>
          <div class="flex-1 md:pl-12 order-1 md:order-2 pl-16 md:pl-12">
            <div class="bg-white rounded-2xl p-6 shadow-lg border border-gray-100 animate-on-scroll">
              <div class="<%= textColors[index] %> font-bold text-lg mb-2"><%= event.date %></div>
              <h3 class="text-xl font-bold text-police-titre mb-2"><%= event.title %></h3>
              <p class="text-police-grey"><%= event.description %></p>
            </div>
          </div>
          <% } %>
        </div>
        <% }); %>
      </div>
    </div>
  </div>
</section>

<!-- Team Section -->
<section class="section bg-white">
  <div class="container-content">
    <div class="text-center mb-16 animate-on-scroll">
      <div class="inline-flex items-center gap-2 px-4 py-2 rounded-full bg-primary/10 border border-primary/20 mb-6">
        <span class="w-2 h-2 bg-primary rounded-full animate-pulse"></span>
        <span class="text-primary font-medium text-sm tracking-wide"><%= locals.about.team.badge %></span>
      </div>
      <h2 class="text-3xl md:text-4xl font-bold text-police-titre mb-4">
        <%= locals.about.team.title %> <span class="text-gradient"><%= locals.about.team.titleHighlight %></span>
      </h2>
      <p class="text-police-grey max-w-2xl mx-auto">
        <%= locals.about.team.subtitle %>
      </p>
    </div>

    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8" data-stagger-scale>
      <% locals.about.team.members.forEach(function(member) { %>
      <div class="group bg-white rounded-2xl p-6 border border-gray-100 hover:border-primary/30 hover:shadow-xl transition-all duration-300">
        <div class="relative mb-8">
          <div class="w-32 h-32 mx-auto rounded-full overflow-hidden border-4 border-gray-100 group-hover:border-primary/30 transition-colors">
            <img src="../static/assets/images/<%= member.image %>" alt="<%= member.name %>" class="w-full h-full object-cover">
          </div>
          <div class="absolute left-1/2 -translate-x-1/2 px-3 py-1 rounded-full <%= member.roleBg %>">
            <span class="<%= member.roleText %> text-xs font-semibold"><%= member.role %></span>
          </div>
        </div>
        <div class="text-center">
          <h3 class="text-xl font-bold text-police-titre mb-1"><%= member.name %></h3>
          <p class="<%= member.titleText %> font-medium text-sm mb-3"><%= member.title %></p>
          <p class="text-police-grey text-sm mb-4"><%= member.description %></p>
          <% if (member.linkedin) { %>
          <a href="<%= member.linkedin %>" target="_blank" rel="noopener noreferrer" class="inline-flex items-center gap-2 text-police-grey hover:text-primary transition-colors">
            <svg class="w-5 h-5" fill="currentColor" viewBox="0 0 24 24">
              <path d="M20.447 20.452h-3.554v-5.569c0-1.328-.027-3.037-1.852-3.037-1.853 0-2.136 1.445-2.136 2.939v5.667H9.351V9h3.414v1.561h.046c.477-.9 1.637-1.85 3.37-1.85 3.601 0 4.267 2.37 4.267 5.455v6.286zM5.337 7.433c-1.144 0-2.063-.926-2.063-2.065 0-1.138.92-2.063 2.063-2.063 1.14 0 2.064.925 2.064 2.063 0 1.139-.925 2.065-2.064 2.065zm1.782 13.019H3.555V9h3.564v11.452zM22.225 0H1.771C.792 0 0 .774 0 1.729v20.542C0 23.227.792 24 1.771 24h20.451C23.2 24 24 23.227 24 22.271V1.729C24 .774 23.2 0 22.222 0h.003z"/>
            </svg>
            <span class="text-sm">LinkedIn</span>
          </a>
          <% } %>
        </div>
      </div>
      <% }); %>
    </div>
  </div>
</section>

<!-- Values Section -->
<section class="section bg-gray-50">
  <div class="container-content">
    <div class="text-center mb-16 animate-on-scroll">
      <div class="inline-flex items-center gap-2 px-4 py-2 rounded-full bg-primary/10 border border-primary/20 mb-6">
        <span class="w-2 h-2 bg-primary rounded-full animate-pulse"></span>
        <span class="text-primary font-medium text-sm tracking-wide"><%= locals.about.values.badge %></span>
      </div>
      <h2 class="text-3xl md:text-4xl font-bold text-police-titre">
        <%= locals.about.values.title %> <span class="text-gradient"><%= locals.about.values.titleHighlight %></span>
      </h2>
    </div>
    <div class="grid grid-cols-1 md:grid-cols-3 gap-8" data-stagger-scale>
      <% var valueIcons = [
        {bg: 'bg-gradient-to-br from-primary/20 to-purple-500/20 text-primary', svg: 'M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z'},
        {bg: 'bg-gradient-to-br from-emerald-500/20 to-teal-500/20 text-emerald-600', svg: 'M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z'},
        {bg: 'bg-gradient-to-br from-amber-500/20 to-orange-500/20 text-amber-600', svg: 'M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z'}
      ]; %>
      <% locals.about.values.items.forEach(function(value, index) { %>
      <div class="card text-center bg-white">
        <div class="w-16 h-16 mx-auto mb-4 flex items-center justify-center rounded-2xl <%= valueIcons[index].bg %>">
          <svg class="w-8 h-8" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="<%= valueIcons[index].svg %>"></path>
          </svg>
        </div>
        <h3 class="text-xl font-semibold text-police-titre mb-3"><%= value.title %></h3>
        <p class="text-police-grey"><%= value.description %></p>
      </div>
      <% }); %>
    </div>
  </div>
</section>

<!-- Technology Section -->
<section class="section bg-white">
  <div class="container-content">
    <div class="grid grid-cols-1 lg:grid-cols-2 gap-16 items-center">
      <div class="order-2 lg:order-1 animate-on-scroll">
        <div class="bg-gradient-to-br from-[#030014] to-primary/20 rounded-3xl p-8 relative overflow-hidden">
          <div class="absolute inset-0 bg-[linear-gradient(rgba(92,57,238,0.05)_1px,transparent_1px),linear-gradient(90deg,rgba(92,57,238,0.05)_1px,transparent_1px)] bg-[size:30px_30px]"></div>
          <div class="relative z-10">
            <h3 class="text-xl font-bold text-white mb-6"><%= locals.about.technology.stackTitle %></h3>
            <ul class="space-y-4">
              <% var stackIcons = [
                {bg: 'bg-primary/20', text: 'text-primary', svg: 'M9 3v2m6-2v2M9 19v2m6-2v2M5 9H3m2 6H3m18-6h-2m2 6h-2M7 19h10a2 2 0 002-2V7a2 2 0 00-2-2H7a2 2 0 00-2 2v10a2 2 0 002 2zM9 9h6v6H9V9z'},
                {bg: 'bg-purple-500/20', text: 'text-purple-400', svg: 'M19 11a7 7 0 01-7 7m0 0a7 7 0 01-7-7m7 7v4m0 0H8m4 0h4m-4-8a3 3 0 01-3-3V5a3 3 0 116 0v6a3 3 0 01-3 3z'},
                {bg: 'bg-emerald-500/20', text: 'text-emerald-400', svg: 'M8 9l3 3-3 3m5 0h3M5 20h14a2 2 0 002-2V6a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z'},
                {bg: 'bg-amber-500/20', text: 'text-amber-400', svg: 'M3 15a4 4 0 004 4h9a5 5 0 10-.1-9.999 5.002 5.002 0 10-9.78 2.096A4.001 4.001 0 003 15z'}
              ]; %>
              <% locals.about.technology.stack.forEach(function(item, index) { %>
              <li class="flex items-center gap-4 text-white/80">
                <div class="w-10 h-10 rounded-xl <%= stackIcons[index].bg %> flex items-center justify-center flex-shrink-0">
                  <svg class="w-5 h-5 <%= stackIcons[index].text %>" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="<%= stackIcons[index].svg %>"></path>
                  </svg>
                </div>
                <span><%= item %></span>
              </li>
              <% }); %>
            </ul>
          </div>
        </div>
      </div>
      <div class="order-1 lg:order-2 animate-on-scroll">
        <div class="inline-flex items-center gap-2 px-3 py-1.5 rounded-full bg-primary/10 border border-primary/20 mb-6">
          <span class="w-1.5 h-1.5 bg-primary rounded-full"></span>
          <span class="text-primary text-xs font-semibold uppercase tracking-wider"><%= locals.about.technology.badge %></span>
        </div>
        <h2 class="text-3xl md:text-4xl font-bold text-police-titre mb-6">
          <%= locals.about.technology.title %> <span class="text-gradient"><%= locals.about.technology.titleHighlight %></span>
        </h2>
        <p class="text-lg text-police-grey mb-4">
          <%= locals.about.technology.paragraph1 %>
        </p>
        <p class="text-police-grey mb-4">
          <%= locals.about.technology.paragraph2 %>
        </p>
        <p class="text-police-grey">
          <%= locals.about.technology.paragraph3 %>
        </p>
      </div>
    </div>
  </div>
</section>

<!-- CTA Section -->
<section class="py-24 md:py-32 bg-[#030014] relative overflow-hidden">
  <!-- Animated background -->
  <div class="absolute inset-0">
    <div class="absolute top-0 left-1/4 w-[600px] h-[600px] bg-gradient-to-r from-primary/30 to-transparent rounded-full blur-[120px] animate-pulse-slow"></div>
    <div class="absolute bottom-0 right-1/4 w-[500px] h-[500px] bg-gradient-to-l from-secondary/20 to-transparent rounded-full blur-[100px] animate-pulse-slow" style="animation-delay: 2s;"></div>
  </div>

  <div class="container-content text-center relative z-10">
    <h2 class="text-3xl md:text-4xl lg:text-5xl font-bold text-white mb-6 animate-on-scroll">
      <%= locals.about.cta.title %>
    </h2>
    <p class="text-xl text-white/60 mb-10 max-w-2xl mx-auto animate-on-scroll">
      <%= locals.about.cta.subtitle %>
    </p>
    <div class="animate-on-scroll">
      <a href="<%= locals.cta.primary.url %>" class="btn-primary inline-flex items-center gap-2 text-lg px-8 py-4" data-track-content data-content-name="CTA Contact" data-content-piece="About Footer CTA">
        <%= locals.about.cta.button %>
        <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path>
        </svg>
      </a>
    </div>
  </div>
</section>

</main>

<%- include('./includes/_footer.ejs') %>

</body>
</html>
