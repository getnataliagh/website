{
  "name": "natalia-website",
  "version": "1.0.0",
  "private": true,
  "description": "Natalia - Assistante Vocale IA pour entreprises",
  "homepage": "https://getnatalia.com/",
  "scripts": {
    "serve": "npx nodemon -w ./data.js -w . -i dist/ -i src/ --ext .js,.json,.ejs --exec 'npm run build:dev && npx serve dist'",
    "build:dev": "parcel build src/*.ejs --no-cache",
    "build": "parcel build src/*.ejs --public-url=/",
    "clean": "rm -rf dist .parcel-cache"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.9.3",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "nodemon": "^3.0.1",
    "parcel": "^2.9.3",
    "parcel-reporter-sitemap": "^1.0.1",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "parcel-transformer-ejs": "^1.0.0",
    "postcss": "^8.4.0",
    "postcss-100vh-fix": "^2.0.0",
    "postcss-import": "^16.0.0",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "lottie-web": "^5.12.2",
    "swiper": "^11.0.5"
  },
  "volta": {
    "node": "22.13.1"
  }
}
